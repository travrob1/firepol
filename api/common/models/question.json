{
  "name": "Question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Question": {
      "type": "string",
      "required": true
    },
    "Summary": {
      "type": "string",
      "required": true
    },
    "Details": {
      "type": "string",
      "required": true
    },
    "Status": {
      "type": "string",
      "required": true,
      "default": "preinvite"
    },
    "ClosingTime": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "Comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "commentId"
    },
    "Facilitator": {
      "type": "hasOne",
      "model": "UserProfile",
      "foreignKey": "Facilitator"
    },
    "Answers": {
      "type": "hasMany",
      "model": "Answer",
      "foreignKey": "answerId"
    }
  },
  "acls": [],
  "methods": {}
}
