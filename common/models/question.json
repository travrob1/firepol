{
  "name": "Question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "question": {
      "type": "string",
      "required": true
    },
    "summary": {
      "type": "string",
      "required": true
    },
    "details": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "preinvite"
    },
    "closingTime": {
      "type": "date"
    },
    "votes": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "responses": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "views": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "Comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": ""
    },
    "Facilitator": {
      "type": "belongsTo",
      "model": "userProfile",
      "foreignKey": "facilitatorUserProfileId"
    },
    "Answers": {
      "type": "hasMany",
      "model": "answer",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
