{
  "name": "question",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Question": {
      "type": "string",
      "required": true
    },
    "Summary": {
      "type": "string",
      "required": true
    },
    "Details": {
      "type": "string",
      "required": true
    },
    "Status": {
      "type": "string",
      "required": true,
      "default": "preinvite"
    },
    "ClosingTime": {
      "type": "date"
    },
    "Votes": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "Responses": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "Views": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "Comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": ""
    },
    "Facilitator": {
      "type": "belongsTo",
      "model": "userProfile",
      "foreignKey": "facilitatorUserProfileId"
    },
    "Answers": {
      "type": "hasMany",
      "model": "answer",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
